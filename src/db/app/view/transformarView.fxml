<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.65"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="db.app.view.transformarViewController">
    <children>
        <VBox alignment="CENTER" layoutX="68.0" layoutY="92.0" prefHeight="400.0" prefWidth="300.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="nombrePersonaje" text="Label">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <ImageView fx:id="imagePersonaje" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true"
                           preserveRatio="true"/>
                <Button mnemonicParsing="false" onAction="#onCLickCambiarEstado" prefHeight="100.0" prefWidth="300.0"
                        text="Transformar !"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
